    "# 例2.求积分\n",
    "from scipy import integrate\n",
    "import math\n",
    "\n",
    "def exp_inte(x, a):\n",
    "    return a*math.pow(x,2)\n",
    "\n",
    "print(integrate.quad(exp_inte,2,6,args=(3,),full_output=1))"
   ]
